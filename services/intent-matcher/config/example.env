# EigenCompute App Environment

# EigenX auth & registry
EIGENX_ENV=mainnet
EIGENX_APP_ID=eigenmatch-intent-matcher

# Encrypted secrets (EigenCompute KMS releases only inside TEE)
POSTGRES_DSN=postgres://matcher:password@db:5432/matcher?sslmode=disable
MATCHER_PRIVATE_KEY=0xabc123
AVS_OPERATOR_SET_JSON=base64:...
EIGENAI_ENDPOINT=https://eigenai.eigenmatch.xyz/score

# Public diagnostics
METRICS_PORT_PUBLIC=0.0.0.0:9090
API_PORT_PUBLIC=0.0.0.0:8080

# EigenCompute specific
MATCH_EPOCH_SECONDS=5
MAX_BUNDLE_DELAY_SECONDS=30
MIN_WATCHER_ACKS=2

